{
  "name": "life-dashboard",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "prepare": "prisma generate",
    "dev:webhooks": "tsx --watch services/webhooks/src/index.ts",
    "dev:jobs": "tsx --watch services/jobs/src/index.ts",
    "dev:llm": "tsx --watch services/llm/src/index.ts",
    "build:webhooks": "tsc -p services/webhooks/tsconfig.json",
    "build:jobs": "tsc -p services/jobs/tsconfig.json",
    "build:llm": "tsc -p services/llm/tsconfig.json",
    "start:webhooks": "tsx services/webhooks/src/index.ts",
    "start:jobs": "tsx services/jobs/src/index.ts",
    "start:llm": "tsx services/llm/src/index.ts"
  },
  "dependencies": {
    "@google-cloud/storage": "^7.9.0",
    "@google-cloud/vertexai": "^0.3.0",
    "@prisma/client": "5.22.0",
    "@supabase/auth-helpers-nextjs": "^0.10.0",
    "@supabase/supabase-js": "^2.43.4",
    "@tanstack/react-query": "^5.33.0",
    "@total-typescript/ts-reset": "^0.5.1",
    "cors": "^2.8.5",
    "clsx": "^2.1.1",
    "date-fns": "^3.6.0",
    "express": "^4.19.2",
    "lucide-react": "^0.378.0",
    "next": "^14.2.33",
    "next-themes": "^0.3.0",
    "pino": "^9.3.2",
    "pino-pretty": "^11.2.2",
    "posthog-js": "^1.128.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "stripe": "^14.24.0",
    "swr": "^2.2.4",
    "tailwind-merge": "^2.2.1",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.3.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/cors": "^2.8.17",
    "@types/node": "^20.12.7",
    "@types/express": "^4.17.21",
    "@types/react": "^18.2.69",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^7.5.0",
    "@typescript-eslint/parser": "^7.5.0",
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "^10.4.17",
    "eslint": "^8.57.0",
    "eslint-config-next": "14.2.3",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "prisma": "^5.15.0",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.4.5",
    "tsx": "^4.10.2",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "packageManager": "pnpm@9.0.0"
}
